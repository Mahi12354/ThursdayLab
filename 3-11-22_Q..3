import java.util.*;
public class fibonathread extends Thread {
Scanner a=new Scanner(System.in);
public void run() {
	int number=1;
	int number1=10;
	int number2 = 0;
	int count=10;
	for(int i=2;i<count;i++)
	{
		number=number1; 
		number1=number2;
		number2=number+number1; //formula
		System.out.println("the fibonacci series of given numberis "+number2);
		 
	}
	int n=10;
	int a[]=new int[n];
    a[0]=0;
    a[1]=1;
    for(int i=2;i<n;i++) {
    	a[i]=a[i-2]+a[i-1]; //formula for reverse
    }
    System.out.println("The Reverse Fibonacci series :");
    for(int i=n-1;i>=0;i--) {
    	System.out.println(a[i]);
    }
}
public static void main(String[] args) {
	fibonathread f=new fibonathread();
	fibonathread f1=new fibonathread();
	f.start();
	f1.start();
}
}
